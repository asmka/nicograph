{
  "name": "nicograph",
  "version": "1.1.0",
  "description": "ニコニコ動画のシークバー上に区間コメント数のグラフを描画するChrome拡張機能",
  "private": true,
  "scripts": {
    "clean": "rimraf dist release release.zip",
    "prebuild": "npm run clean",
    "build": "tsc && webpack -o release/ && ncp images/icons release/icons && ncp manifest.json release/manifest.json",
    "postbuild": "zip -r release.zip release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dff9th/nicograph.git"
  },
  "author": "asmka",
  "license": "MIT",
  "volta": {
    "node": "16.15.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^7.32.0",
    "ncp": "^2.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
