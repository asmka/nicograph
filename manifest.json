{
    "name": "Niconico Comment Grapher",
    "version": "0.2.0",
    "description": "Graphing interval comments over seek bar",
    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "*://nicovideo.cdn.nimg.jp/web/scripts/pages/watch/watch_app_*.js"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [{
        "matches": [
            "*://www.nicovideo.jp/watch/*"
        ],
        "js": [
            "js/index.js"
        ]
    }],
    "background": {
        "scripts": ["js/background.js"]
    },
    "web_accessible_resources": [
        "js/hack_fetch_thread.js"
    ],
    "page_action": {
        "default_popup": "",
        "default_icon": {
        }
    },
    "icons": {
    },
    "manifest_version": 2
}
