{
  "name": "Niconico Comment Grapher",
  "version": "0.1",
  "description": "graphing the number of comments over seek bar",
  "permissions": [
    "https://api.nicoad.nicovideo.jp/"
  ],
  "background": {
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["http://www.nicovideo.jp/watch/sm*", "https://www.nicovideo.jp/watch/sm*"],
    "js": ["js/acscmtsvr.js", "js/acsadssvr.js", "js/main.js"]
  }]
}
